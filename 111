import urllib.request
import re

def loadurl(url):
   text = urllib.request.urlopen(url).read().decode('GBK')
   extract(text)

def extract(text):
	#print(text)
	airline_re = r'<div class="base_main" .*?>(.*?)</div>'
	airline_set = re.findall(airline_re,text)
	#print(airline_set)
	for(airline) in zip(airline_set):
		print(airline)

if __name__ == "__main__":
	url='''http://flights.ctrip.com/booking/bjs-xmn-day-1.html?ddate1=2017-02-07'''
	loadurl(url)